
@{
    ViewData["Title"] = "Index";
}

<h2>Send input to service bus queue!</h2>

<p>
    Choose one.<br />
    @Html.ActionLink("Send 6 large sized messages to the queue", "SendMessage", new { size = "large" })<br />
    @Html.ActionLink("Send 48 medium sized messages to the queue", "SendMessage", new { size = "medium" })<br />
    @Html.ActionLink("Send 96 small sized messages to the queue", "SendMessage", new { size = "small" })
    <br /><br />
    @Html.Raw(@ViewBag.Result)
</p>